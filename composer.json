{
  "name": "spryker/merchant-storage",
  "type": "library",
  "description": "MerchantStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/event-behavior": "^1.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/merchant": "^2.0.0",
    "spryker/propel-orm": "*",
    "spryker/storage": "^3.4.0",
    "spryker/synchronization": "^1.0.0",
    "spryker/synchronization-behavior": "^1.0.0",
    "spryker/synchronization-extension": "^1.0.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/event": "*",
    "spryker/url-storage": "*"
  },
  "suggest": {
    "spryker/event": "ADD SUGGEST DESCRIPTION",
    "spryker/url-storage": "To use shop route plugin."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
